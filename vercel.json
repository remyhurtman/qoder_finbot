{
  "version": 2,
  "builds": [
    {
      "src": "api/test.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/health.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/webhook.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/set-webhook.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/test_token.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/fix_webhook.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/simple_webhook.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/reset_webhook.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/test",
      "dest": "api/test.py"
    },
    {
      "src": "/health",
      "dest": "api/health.py"
    },
    {
      "src": "/api/webhook",
      "dest": "api/simple_webhook.py"
    },
    {
      "src": "/api/set-webhook",
      "dest": "api/set-webhook.py"
    },
    {
      "src": "/api/test-token",
      "dest": "api/test_token.py"
    },
    {
      "src": "/api/fix-webhook",
      "dest": "api/fix_webhook.py"
    },
    {
      "src": "/api/old-webhook",
      "dest": "api/webhook.py"
    },
    {
      "src": "/api/reset-webhook",
      "dest": "api/reset_webhook.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}