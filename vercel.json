{
  "version": 2,
  "builds": [
    {
      "src": "api/test.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/health.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/test",
      "dest": "api/test.py"
    },
    {
      "src": "/health",
      "dest": "api/health.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}